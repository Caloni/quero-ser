#include <iostream>

using namespace std;
int main()
{
    int n;

    cout << "Defina o tamanho dos vetores: ";
    cin >> n;
    double vetor1[n], vetor2[n], vetorf[n];
    cout << "Digite o vetor 1: ";

    for(int i = 0; i<n; i++)
    {
        cin >> vetor1[i];
    }

    cout << "Digite o vetor 2: ";
    for(int i = 0; i<n; i++)
    {
        cin >> vetor2[i];
    }
    for(int i = 0; i<n; i++)
    {
        vetorf[i] += vetor1[i] * vetor2[i];
    }
    cout << "Vetor escalar: ";
    for(int i = 0; i<n; i++)
    {
        cout << vetorf[i] << " ";
    }

}
